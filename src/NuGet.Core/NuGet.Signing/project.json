{
    "version": "4.0.0-*",
    "description": "NuGet's implementation for package signing.",
    "authors": [
        "NuGet"
    ],
    "copyright": "Copyright .NET Foundation. All rights reserved.",
    "packOptions": {
        "tags": [
            "signing",
            "package signing"
        ],
        "licenseUrl": "https://raw.githubusercontent.com/NuGet/NuGet.Client/dev/LICENSE.txt",
        "projectUrl": "https://github.com/NuGet/NuGet.Client"
    },
    "buildOptions": {
        "warningsAsErrors": true,
        "xmlDoc": true,
        "nowarn": [
            "CS1591",
            "CS1574",
            "CS1573"
        ],
        "compile": {
            "include": [
                "../../../Shared/*.cs"
            ]
        }
    },
    "dependencies": {
        "System.Security.Cryptography.Primitives": "4.3.0",
        "NuGet.Common": {
            "target": "project"
        },
        "NuGet.Packaging": {
            "target": "project"
        },
        "NuGet.Versioning": {
            "target": "project"
        }
    },
    "frameworks": {
        "net46": {
            "frameworkAssemblies": {
            },
            "buildOptions": {
                "define": [
                    "IS_DESKTOP"
                ]
            },
            "dependencies": {
                "System.Security.Cryptography.Encoding": "4.3.0",
                "System.Security.Cryptography.Pkcs": "4.3.0",
                "System.Security.Cryptography.X509Certificates": "4.3.0"
            }
        }
    }
}